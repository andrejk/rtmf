		<restriction base="StUF:ParametersVraag">
			<sequence>
				<element name="sortering">
					<annotation>
						<documentation>
								Sortering 1: code, ingangsdatum (aflopend)
								Sortering 2: omschrijving, ingangsdatum (aflopend)
								Sortering 3: ingangsdatum (aflopend), code
							</documentation>
					</annotation>
					<simpleType>
						<restriction base="StUF:Sortering">
							<minInclusive value="0"/>
							<maxInclusive value="3"/>
						</restriction>
					</simpleType>
				</element>
				<element name="indicatorVervolgvraag" type="boolean" default="false"/>
				<element name="maximumAantal" type="StUF:MaximumAantal" default="15" minOccurs="0"/>
				<element name="indicatorHistorisch" type="StUF:IndicatorHistorisch" minOccurs="0"/>
				<element name="indicatorAfnemerIndicatie" type="boolean" default="false" minOccurs="0"/>
				<element name="indicatorAantal" type="boolean" default="false" minOccurs="0"/>
			</sequence>
		</restriction>
	</complexContent>
</complexType>
