			<value>http://192.168.21.1:80/services</value>
		</property>
	</bean>
	<bean id="attrInvMallFormedURL"
		class="com.ovsoftware.ictu.osb.tmfportal.service.stelselcatalogus.impl.wus.wuslite.WWLAttribuutInvokerImpl">
